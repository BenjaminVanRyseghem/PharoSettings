StartupLoader default executeAtomicItems: {	StartupAction name: 'Nautilus' code: [Smalltalk		at: #Nautilus		ifPresent: [| package |			SystemBrowser default: Nautilus.			package := RPackageOrganizer default packageNamed: 'Nautilus'.			Nautilus groupsManager				addADynamicGroupSilentlyNamed: 'Nautilus'				block: [package orderedClasses].			Nautilus openOnGroups: true]		ifAbsent: [ false ]] runOnce: true.}.